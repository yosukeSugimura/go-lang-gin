# サーバー設定
PORT=8080
GIN_MODE=debug         # 本番環境では "release" に設定

# データベース設定
DB_HOST=localhost       # データベースホスト
DB_PORT=5432            # データベースポート
DB_USER=myuser          # データベースユーザー名
DB_PASSWORD=mypassword  # データベースパスワード
DB_NAME=mydatabase      # データベース名

# 外部API設定
API_KEY=your_api_key    # 外部API用のキー

# プロキシ設定
TRUSTED_PROXY=192.168.1.1  # 信頼できるプロキシのIPアドレス（必要に応じて設定）
